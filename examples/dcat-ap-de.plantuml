        '@ sh:shapes       = dcatap:CatalogRecord_Property_dct_issued (erlaubt auch xsd:gYear und xsd:gYearMonth)

    <<optional>> dct:title ~> rdfs:Literal [*]
        '@ rdfs:label      = Titel
